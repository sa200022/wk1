{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "Database": {
    "ConnectionString": "Server=localhost;Port=3306;Database=webhook_delivery;Uid=job_worker;Pwd=your_password_here;CharSet=utf8mb4;"
